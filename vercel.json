{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python",
        "config":{"runtime":"python3.10.11"}
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  